{
  "name": "soma-backend",
  "version": "1.0.0",
  "description": "a language project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "NODE_ENV=production nodemon --exec babel-node ./src/index.js",
    "start:app": "NODE_ENV=production npm run db-migrate && NODE_ENV=production babel-node ./src/index.js",
    "dev-start": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "db-migrate": "npx sequelize db:migrate",
    "demigrate": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "test": "NODE_ENV=test npm run migrate && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 3000 ./src/tests/mainTest.test.js --exit",
    "coverage": "npm run test  && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soma-Technologies-Inc/somafri-backend.git"
  },
